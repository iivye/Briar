briarmarket {
    hook open
    help Opens the Briar GUI.
    user player
}

bmadmin {
  reload {
    hook reload
    help Reloads the Briar configuration.
  }
  forcerotate player:player {
    hook forcerotate
    help Forcefully rotate provided player's market shop.
  }
  forceglobalrotate {
    hook forceglobalrotate
    help Forcefully rotates all shops.
  }
}