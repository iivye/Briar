briarmarket {
    hook open
    help Opens the BriarMarket GUI.
    user player
}

bmadmin {
  reload {
    hook reload
    help Reloads BriarMarket's configuration.
  }
  rotate player:player {
    hook rotate
    help Rotate the provided player's market.
  }
  globalrotate {
    hook globalrotate
    help Rotates the global market.
  }
}